<connectionStrings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <add name="SqlServer" connectionString="Data Source=%SqlServerAddress%;Initial Catalog=master;User Id=%SqlServerUser%;Password=%SqlServerPassword%;TrustServerCertificate=False;Connection Timeout=300;" providerName="system.data.sqlclient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  <add name="OleDb" connectionString="Provider=SQLNCLI11;Password=%SqlServerPassword%;User ID=%SqlServerUser%@%SqlServerAddress%;Initial Catalog=master;Data Source=tcp:%SqlServerAddress%;Connection Timeout=300;" providerName="system.data.oledb" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  <add name="ODBC" connectionString="Driver={ODBC Driver 13 for SQL Server};Server=tcp:%SqlServerAddress%,1433;Database=master;Uid=%SqlServerUser%@%SqlServerAddress%;Pwd=%SqlServerPassword%;TrustServerCertificate=no;Connection Timeout=300;" providerName="system.data.odbc" xdt:Transform="Replace" xdt:Locator="Match(name)" />
</connectionStrings>
